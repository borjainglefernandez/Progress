# This is the third spreadsheet screen
<WorkoutScreen3>:
    FloatLayout:
        # Goes back to home
        Button:
            pos_hint: {"top": 1, "left": 0.5}
            size_hint: 0.12,0.12
            #Credit: https://www.flaticon.com/authors/xnimrodx
            background_normal: "icons/home.png"
            on_release:
                app.change_screen("home_screen","previous")

        # Collects all the data for the spreadsheet
        Button:
            pos_hint: {"top": 1, "right": 1}
            size_hint: 0.12,0.12
            text: "Submit"
            on_release:
                app.collect_data("workout_screen_3")
